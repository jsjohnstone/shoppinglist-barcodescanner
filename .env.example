# Barcode Scanner Environment Configuration
# Copy this file to .env and configure for your environment

# Backend URL - IP address of your Shopping List server
BACKEND_URL=http://192.168.1.100:3000

# Logging level: error, warn, info, debug
LOG_LEVEL=info

# Test mode - set to true for testing without actual scanner hardware
TEST_MODE=false

# Node environment
NODE_ENV=production
